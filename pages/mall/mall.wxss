.likelist{
  width: 100%;
  height: 100%;
}
.likelist-title{
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  text-align: center;
}
.likelist-nav{
  width: 100%;
  height: 100%;
  display: flex;
  flex-wrap: wrap;
  justify-content: space-around;
}
.item{
  width: 45%;
  height: 381rpx;
  margin-top: 30rpx;
}
.item image{
  width: 350rpx;
  height: 200rpx;
}
.item-price{
  width: 100%;
  height: 50rpx;
  line-height: 50rpx;
  font-size: 32rpx;
  color: #c81c28;
}
.item-desc{
  width: 100%;
  height: 50rpx;
  line-height: 50rpx;
  font-size: 32rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.item-num{
  width: 100%;
  height: 50rpx;
  line-height: 50rpx;
  font-size: 32rpx;
  color: #c0bbbb;
}
page{
  height: 100%;
}
.scroll-list{
  height: 100%;
}