page{
  width: 100%;
  overflow: hidden;
}
.mine{
  width: 100%;
  height: 100%;
  background: #f5f3f3;
}
.mine-title{
  width: 100%;
  height: 400rpx;
  background: rgb(166, 147, 184);
}
.mine-title-name{
  width: 100%;
  height: 300rpx;
  display: flex;
  justify-content: space-between;
   padding: 0 30rpx;
}
.info{
  display: flex;
}
.pic{
  width: 150rpx;
  height: 150rpx;
  border-radius: 50%;
  margin-top: 70rpx;
  margin-right: 30rpx;
}
.login-btn{
  width: 500rpx;
  height: 200rpx;
  margin-top: 100rpx;
  margin-left: 200rpx;
}
.login-btn view{
  width: 100%;
  height: 90rpx;
  font-size: 30rpx;
  color: white;
}
.user-info{
  width: 300rpx;
  height: 50%;
  margin-top: 100rpx;
  line-height: 50rpx;
  font-size: 34rpx;
  color: rgb(11, 43, 61);
}
.nav-icon{
  width: 22%;
  height: 80%;
  margin-top: 20rpx;
  margin-right: 10rpx;
  display: flex;
}
.nav-icon image{
  width: 50rpx;
  height: 50rpx;
  padding: 12rpx;
}
.icon{
  width: 100%;
  height: 120rpx;
  display: flex;
  justify-content: space-around;
  align-items: center;
  background: rgb(128, 93, 158);
  color: white;
}
.login-btn .nonelogined{
  width: 160rpx;
  height: 160rpx;
  border-radius: 50%;
  overflow: hidden;
  position: absolute;
  top:105rpx;
  left:29rpx;
}
.nonelogined image{
  width: 100%;
  height: 100%;
}
.icon view{
  width: 150rpx;
  height: 100rpx;
  display: flex;
  flex-direction: column;
  text-align: center;
  font-size: 30rpx;
  align-items: center;
}
.icon view image{
  width: 50rpx;
  height: 50rpx;
  margin-bottom: 10rpx;
}
.order{
  width: 100%;
  height: 250rpx;
  margin-top: 30rpx;
  padding-left:10rpx;
  background: white;
}
.order-title{
  display: flex;
  align-items: center;
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  border-bottom: 1px solid #dedede;
}
.order-title image{
  width: 50rpx;
  height: 50rpx;
  margin-right: 20rpx;
}
.order-nav{
  display: flex;
  width: 100%;
  height: 150rpx;
  align-items: center;
  justify-content: space-around;
}
.order-nav view{
  width: 159rpx;
  display: flex;
  flex-direction: column;
  text-align: center;
  font-size: 30rpx;
  align-items: center;
}
.order-nav view image{
  width: 50rpx;
  height: 50rpx;
  margin-bottom: 10rpx;
}
.order1{
  width: 100%;
  height: 100rpx;
  margin-top: 30rpx;
  padding: 0 30rpx;
  background: white;
}
.order1-title{
  display: flex;
  align-items: center;
  height: 80rpx;
  line-height: 80rpx;
}
.order1-title image{
  width: 50rpx;
  height: 50rpx;
  margin-right: 20rpx;
}
.module{
  width: 260rpx;
  height: 331rpx;
  background: white;
  position: absolute;
  top: 80rpx;
  right: 0;
  display: flex;
  flex-direction: column;
  border-radius: 5px;
  padding-top: 29rpx;
  border: 1px solid rgb(72, 44, 100);
}
.module view{
  width: 260rpx;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  color: rgb(72, 44, 100);
  border-bottom: 1px solid rgb(72, 44, 100);
}
.module .cancelBtn{
  width: 100rpx;
  height: 60rpx;
  position: absolute;
  top: -10rpx;
  right: -20rpx;
  border: none;
}
.module .cancelBtn image{
  width: 40rpx;
  height: 40rpx;
}